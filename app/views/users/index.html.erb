<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Website</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.phone %></td>
      <td><%= user.website %></td>
    </tr>
  <% end %>
</table>
<h2>Import Users</h2>

<%= form_tag import_users_path, multipart: true do %>
  <%= file_field_tag :csv %>
  <%= submit_tag "Import" %>
<% end %>